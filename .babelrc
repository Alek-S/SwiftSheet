{
	"presets": [
		[
			"@babel/preset-env",
			{
				"targets": {
					"browsers": ["last 2 versions"]
				}
			}
		],
		"@babel/preset-react"
	],
	"plugins": [
		"@babel/plugin-proposal-class-properties",
		"babel-plugin-styled-components",
		"react-hot-loader/babel",
		["@babel/plugin-transform-runtime",
			{
			  "regenerator": true
			}
		  ]
	]
}
