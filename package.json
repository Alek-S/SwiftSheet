{
  "name": "swiftsheet",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "webpack --progress --config ./webpack.prod.js",
    "watch": "webpack --config ./webpack.dev.js -w",
    "precommit": "lint-staged && npm run test",
    "start:webpack-dev": "webpack-dev-server --config ./webpack.dev.js --hot",
    "start:dev": "nodemon --inspect ./server.js",
    "start:prod": "NODE_ENV=production node ./server.js",
    "clean-install": "rm -rf ./node_modules && npm i",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alek-S/SwiftSheet.git"
  },
  "author": "Alek Shnayder",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Alek-S/SwiftSheet/issues"
  },
  "homepage": "https://github.com/Alek-S/SwiftSheet#readme",
  "dependencies": {
    "@sentry/browser": "^5.2.1",
    "@sentry/node": "^5.2.0",
    "ag-grid-community": "^20.2.0",
    "ag-grid-react": "^20.2.0",
    "apollo-boost": "^0.1.27",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "date-fns": "^1.29.0",
    "enzyme-to-json": "^3.3.4",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "express-rate-limit": "^3.5.1",
    "graphql": "^14.1.1",
    "graphql-depth-limit": "^1.1.0",
    "graphql-iso-date": "^3.5.0",
    "graphql-tag": "^2.10.1",
    "graphql-type-json": "^0.2.1",
    "mongoose": "^5.4.11",
    "morgan": "^1.9.1",
    "pako": "^1.0.10",
    "papaparse": "^4.6.3",
    "prop-types": "^15.7.1",
    "raf": "^3.4.0",
    "react": "^16.8.1",
    "react-apollo": "^2.4.1",
    "react-dom": "^16.8.1",
    "react-dropzone": "^4.3.0",
    "react-ga": "^2.5.7",
    "react-hot-loader": "^4.6.5",
    "react-router-dom": "^4.3.1",
    "shortid": "^2.2.14",
    "styled-components": "^3.4.2",
    "waait": "^1.0.4",
    "snyk": "^1.228.5"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-jest": "23.6.0",
    "babel-loader": "7.1.5",
    "babel-plugin-styled-components": "1.10.6",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "codecov": "3.1.0",
    "css-loader": "2.1.0",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.9.1",
    "eslint-plugin-react-hooks": "1.0.1",
    "html-webpack-plugin": "3.2.0",
    "html-webpack-template": "6.2.0",
    "husky": "0.14.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "jest-localstorage-mock": "2.4.0",
    "jest-styled-components": "6.3.1",
    "lint-staged": "7.3.0",
    "mini-css-extract-plugin": "0.5.0",
    "nodemon": "1.18.10",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "preload-webpack-plugin": "3.0.0-beta.3",
    "prettier": "1.16.4",
    "react-test-renderer": "16.8.1",
    "react-testing-library": "5.6.1",
    "redux-devtools-extension": "2.13.8",
    "resource-hints-webpack-plugin": "0.0.2",
    "style-loader": "0.23.1",
    "supertest": "3.4.2",
    "uglifyjs-webpack-plugin": "1.3.0",
    "webpack": "4.29.3",
    "webpack-bundle-analyzer": "3.3.2",
    "webpack-cli": "3.2.3",
    "webpack-dev-server": "3.1.14"
  },
  "lint-staged": {
    "*.js": [
      "prettier --config ./.prettierrc --write",
      "git add"
    ]
  },
  "jest": {
    "coverageDirectory": "./test/coverage/",
    "collectCoverage": true,
    "setupFiles": [
      "raf/polyfill",
      "./test/jest.setup.js",
      "jest-localstorage-mock"
    ],
    "testPathIgnorePatterns": [
      "__snapshots__"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.jsx?$",
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "snyk": true
}
