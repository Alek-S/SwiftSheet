    "helmet": "^3.23.3",
    "mongoose": "^5.9.21",
    "morgan": "^1.10.0",
    "pako": "^1.0.11",
    "papaparse": "^5.2.0",
    "prop-types": "^15.7.1",
    "raf": "^3.4.0",
    "react": "^16.13.1",
    "react-apollo": "^3.1.5",
    "react-dom": "^16.13.1",
    "react-dropzone": "^11.0.1",
    "react-ga": "^3.0.0",
    "react-hot-loader": "^4.13.1",
