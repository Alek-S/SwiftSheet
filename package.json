  "bugs": {
    "url": "https://github.com/Alek-S/SwiftSheet/issues"
  },
  "homepage": "https://github.com/Alek-S/SwiftSheet#readme",
  "dependencies": {
    "@sentry/browser": "^5.19.0",
    "@sentry/node": "^5.19.0",
    "ag-grid-community": "^23.2.1",
    "ag-grid-react": "^23.2.1",
    "apollo-boost": "^0.4.9",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.18.3",
    "chalk": "^4.1.0",
    "date-fns": "^2.14.0",
    "enzyme-to-json": "^3.5.0",
    "express": "^4.16.3",
    "express-graphql": "^0.9.0",
    "express-rate-limit": "^5.1.3",
