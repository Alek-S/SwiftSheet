{
  "name": "swiftsheet",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "webpack --progress --config ./webpack.prod.js",
    "watch": "webpack --config ./webpack.dev.js -w",
    "precommit": "lint-staged && npm run test",
    "start:webpack-dev": "webpack-dev-server --config ./webpack.dev.js --hot",
    "start:dev": "nodemon --inspect ./server.js",
    "start:prod": "NODE_ENV=production node ./server.js",
    "clean-install": "rm -rf ./node_modules && npm i",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alek-S/SwiftSheet.git"
  },
  "author": "Alek Shnayder",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Alek-S/SwiftSheet/issues"
  },
  "homepage": "https://github.com/Alek-S/SwiftSheet#readme",
  "dependencies": {
    "@sentry/browser": "5.6.3",
    "@sentry/node": "^5.2.0",
    "ag-grid-community": "^21.2.2",
    "ag-grid-react": "^21.2.2",
    "apollo-boost": "^0.4.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "date-fns": "^2.4.1",
    "enzyme-to-json": "^3.3.4",
    "express": "^4.16.3",
    "express-graphql": "^0.9.0",
    "express-rate-limit": "^5.0.0",
    "graphql": "^14.1.1",
    "graphql-depth-limit": "^1.1.0",
    "graphql-iso-date": "^3.5.0",
    "graphql-tag": "^2.10.1",
    "graphql-type-json": "^0.3.0",
    "helmet": "^3.21.2",
    "mongoose": "^5.7.5",
    "morgan": "^1.9.1",
    "pako": "^1.0.10",
    "papaparse": "^5.1.0",
    "prop-types": "^15.7.1",
    "raf": "^3.4.0",
    "react": "16.10.2",
    "react-apollo": "^3.1.2",
    "react-dom": "16.10.2",
    "react-dropzone": "^10.1.10",
    "react-ga": "^2.5.7",
    "react-hot-loader": "^4.6.5",
    "react-router-dom": "^5.1.2",
